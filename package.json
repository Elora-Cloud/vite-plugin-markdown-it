{
  "name": "@elora-cloud/vite-plugin-markdown-it",
  "version": "1.0.4",
  "type": "module",
  "description": "this is base vite parser markdown for markdown-it",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "example/src/components",
    "example/src/style",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*.md": "./",
    "./example/src/components/*": "./example/src/components/*",
    "./example/src/style/*": "./example/src/style/*",
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "repository": {
    "type": "gitee",
    "url": "https://gitee.com/Elora_Cloud/vite-plugin-markdown-it"
  },
  "scripts": {
    "test": "vitest -u",
    "dev:example": "vite dev example",
    "build:example": "vite build example",
    "preview:example": "vite preview example",
    "release": "bumpp",
    "build:dts": "rimraf -rf ./dist/components  && vue-tsc --declaration --skipLibCheck --emitDeclarationOnly ./example/src/components/index.ts --outDir ./dist/components",
    "build": "rimraf -rf /dist && tsup"
  },
  "keywords": [],
  "author": "Mumulx",
  "license": "ISC",
  "dependencies": {
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-class": "^1.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-toc-done-right": "^4.2.0",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.0.3",
    "@mistjs/tsconfig-vue": "^0.0.3",
    "@toycode/markdown-it-class": "^1.2.4",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-emoji": "^2.0.4",
    "@types/node": "^18.19.5",
    "@types/uniqid": "^5.3.4",
    "@vitejs/plugin-vue": "^4.6.2",
    "bumpp": "^8.2.1",
    "changelogen": "^0.4.1",
    "element-plus": "^2.4.4",
    "eslint": "^8.56.0",
    "highlight.js": "^11.9.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.7",
    "tsup": "^6.7.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.24.1",
    "vite-plugin-inspect": "^0.7.42",
    "vitest": "^0.34.6",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.5.0",
    "vite": "^4.0.0 || ^5.0.0",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
  "directories": {
    "example": "example",
    "test": "tests"
  }
}
