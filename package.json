{
  "name": "vite-plugin-markdown-it",
  "version": "1.0.0",
  "description": "this is base vite parser markdown for markdown-it",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme": {
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js",
      "types": "./dist/theme.d.ts"
    },
    "./package.json": "./package.json",
    "./README.md": "./README.md",
    "./CHANGELOG.md": "./CHANGELOG.md",
    "./README.zh-CN.md": "./README.zh-CN.md",
    "./example/src/components/*": "./dist/components/*",
    "./example/src/style/*": "./dist/style/*",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "test": "vitest -u",
    "dev:example": "vite dev example",
    "build:example": "vite build example",
    "preview:example": "vite preview example",
    "release": "bumpp",
    "build": "rimraf -rf /dist && tsup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/markdown-it-emoji": "^2.0.2",
    "loader-utils": "^2.0.4",
    "markdown-it": "^13.0.1",
    "markdown-it-class": "^1.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "rimraf": "^5.0.1",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.0.3",
    "@mistjs/tsconfig-vue": "^0.0.3",
    "@toycode/markdown-it-class": "^1.2.4",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/node": "^18.16.16",
    "@types/uniqid": "^5.3.2",
    "@vitejs/plugin-vue": "^3.2.0",
    "bumpp": "^8.2.1",
    "changelogen": "^0.4.1",
    "element-plus": "^2.3.5",
    "eslint": "^8.41.0",
    "highlight.js": "^11.8.0",
    "sass": "^1.62.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.28",
    "vitest": "^0.25.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "peerDependencies": {
    "vite": "^3.0.0 || ^4.0.0",
    "vitepress": "^1.0.0-alpha.20"
  }
}
